{
  "manifest_version": 3,
  "name": "memory-cards-extension",
  "description": "memory-cards-extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "memory-cards-extension",
    "default_icon": {
      "16": "./src/icons/get_started16.png",
      "32": "./src/icons/get_started32.png",
      "48": "./src/icons/get_started48.png",
      "128": "./src/icons/get_started128.png"
    }
  },
  "icons": {
    "16": "./src/icons/get_started16.png",
    "32": "./src/icons/get_started32.png",
    "48": "./src/icons/get_started48.png",
    "128": "./src/icons/get_started128.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "identity"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "oauth2": {
    "client_id": "710523153986-0n99ds13p874jqf65hjh59n3mdfriecs.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}
